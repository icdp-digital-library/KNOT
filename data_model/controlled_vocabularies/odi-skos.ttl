@prefix odi: <https://purl.org/ebr/odi#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://purl.org/ebr/odi#> a skos:ConceptScheme, owl:Ontology ;
    rdfs:label "ODI" ;
    rdfs:comment "This is a test" .

odi:Condition a owl:Class ;
    skos:inScheme <https://purl.org/ebr/odi#> ;
    skos:topConceptOf <https://purl.org/ebr/odi#> ;
    rdfs:label "Condition"@en,
        "Condizione"@it ;
    skos:prefLabel "Condition"@en,
        "Condizione"@it ;
    rdfs:comment "Is the current phisically condition of a card."@en,
        "È lo stato fisico attuale di una carta."@it ;
    rdfs:subClassOf skos:Concept .

odi:arrivesAt a owl:ObjectProperty ;
    skos:inScheme <https://purl.org/ebr/odi#> ;
    skos:topConceptOf <https://purl.org/ebr/odi#> ;
    rdfs:label "Arrives at"@en,
        "Arriva a"@it ;
    skos:prefLabel "Arrives at"@en,
        "Arriva a"@it ;
    rdfs:comment "It means that a main character arrives at a place in his story."@en,
        "Significa che un protagonista arriva in un luogo nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/FictionalPlace> ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:subClassOf skos:Concept .

<https://w3id.org/odi/bumpsInto> a owl:ObjectProperty ;
    rdfs:label "Bumps into"@en,
        "Si imbatte in"@it ;
    rdfs:comment "It means that a main character meets by chance another character during his story."@en,
        "Significa che un protagonista incontra per caso un altro personaggio durante la sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/carriedOutby> a owl:ObjectProperty ;
    rdfs:label "Carried out by"@en,
        "È eseguito da"@it ;
    rdfs:comment "An event is carried out by one or more characters."@en,
        "Un evento viene realizzato da uno o più personaggi."@it ;
    rdfs:domain <https://w3id.org/odi/Event> ;
    rdfs:range <https://w3id.org/odi/Character> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/carriesRepresentation> a owl:ObjectProperty ;
    rdfs:label "Carries representation"@en,
        "Porta la rappresentazione"@it ;
    rdfs:comment "It shows what represent a card in a story."@en,
        "Mostra cosa rappresenta una carta in una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Storycard> ;
    rdfs:range <https://w3id.org/odi/Representation> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/changesIn> a owl:ObjectProperty ;
    rdfs:label "Changes in"@en,
        "Cambia in"@it ;
    rdfs:comment "This property indicates that a character is represented by two cards representing a different way of being in the same story."@en,
        "Questa proprietà indica che un personaggio è rappresentato da due carte che rappresentano un suo diverso modo di essere nella stessa storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/climbs> a owl:ObjectProperty ;
    rdfs:label "Climbs"@en,
        "Si arrampica"@it ;
    rdfs:comment "This property indicates that a main character climbs a log in his story."@en,
        "Questa proprietà indica che un protagonista si arrampica su un tronco nella sua storia."@it ;
    rdfs:range <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/contains> a owl:ObjectProperty ;
    rdfs:label "Contains"@en,
        "Contiene"@it ;
    rdfs:comment "It connects the tarot deck with its constituent cards."@en,
        "Collega il mazzo di tarocchi con le carte che lo compongono."@it ;
    rdfs:domain <https://w3id.org/odi/TarotDeck> ;
    rdfs:range <https://w3id.org/odi/DeckCard> ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:inverseOf <https://w3id.org/odi/isContainedIn> .

<https://w3id.org/odi/discovers> a owl:ObjectProperty ;
    rdfs:label "Discovers"@en,
        "Scopre"@it ;
    rdfs:comment "It connects a main character with another character or an event whose true identity or existence is discovered by him during his story."@en,
        "Mette in relazione un protagonista con un altro personaggio o evento la cui vera identità o esistenza viene scoperta da lui nel corso della sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character>,
        <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasAParticularConditionOf> a owl:ObjectProperty ;
    rdfs:label "Has a particular condition of"@en,
        "Ha una particolare condizione di"@it ;
    rdfs:comment "It shows a particular condition of the main character in his story."@en,
        "Mostra una condizione particolare del protagonista nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Symbol> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasAuthor> a owl:ObjectProperty ;
    rdfs:label "Has author"@en,
        "Ha autore"@it ;
    rdfs:comment "This property makes explicit the author of both the tarot deck and a specific card in the tarot deck."@en,
        "Questa proprietà rende esplicito l'autore sia del mazzo di tarocchi sia di una specifica carta del mazzo."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard>,
        <https://w3id.org/odi/Edition>,
        <https://w3id.org/odi/TarotDeck> ;
    rdfs:range <https://w3id.org/odi/Person> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasCard> a owl:ObjectProperty ;
    rdfs:label "Has card"@en,
        "Ha la carta"@it ;
    rdfs:comment "It connects each story with its cards."@en,
        "Collega ogni storia alle sue carte."@it ;
    rdfs:domain <https://w3id.org/odi/Story> ;
    rdfs:range <https://w3id.org/odi/Storycard> ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:inverseOf <https://w3id.org/odi/isACardOf> .

<https://w3id.org/odi/hasChapter> a owl:ObjectProperty ;
    rdfs:label "Has chapter"@en,
        "Ha capitolo"@it ;
    rdfs:comment "This property shows a chapter of the edition of reference."@en,
        "Questa proprietà mostra un capitolo dell'edizione di riferimento."@it ;
    rdfs:domain <https://w3id.org/odi/Edition> ;
    rdfs:range <https://w3id.org/odi/Chapter> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasCurrentLocation> a owl:ObjectProperty ;
    rdfs:label "Has current location"@en,
        "È attualmente conservata"@it ;
    rdfs:comment "It indicates the physical location where each card in the deck is stored.",
        "Indica il luogo in cui è attualmente conservata ogni carta del mazzo."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard> ;
    rdfs:range <https://w3id.org/odi/Place> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasCurrentNumberOfCards> a owl:DatatypeProperty ;
    rdfs:label "Has current number of cards"@en,
        "Ha numero attuale di carte"@it ;
    rdfs:comment "It indicates the current number of cards in the tarot deck."@en,
        "Indica il numero attuale di carte del mazzo di tarocchi."@it ;
    rdfs:domain <https://w3id.org/odi/TarotDeck> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasDate> a owl:DatatypeProperty ;
    rdfs:label "Has date"@en,
        "Ha data"@it ;
    rdfs:comment "It indicates the century the tarot deck dates back to."@en,
        "Indica il secolo a cui risale il mazzo di tarocchi."@it ;
    rdfs:domain <https://w3id.org/odi/TarotDeck> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasFamilyRelationWith> a owl:ObjectProperty ;
    rdfs:label "Has family relation with"@en,
        "Ha una relazione familiare con"@it ;
    rdfs:comment "It means that a main character has got a family relation with an another character in his story."@en,
        "Significa che un protagonista ha una relazione familiare con un altro personaggio nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasIconography> a owl:ObjectProperty ;
    rdfs:label "Has iconography"@en,
        "Ha iconografia"@it ;
    rdfs:comment "It indicates the dimension of each card's iconography in the page of the book."@en,
        "Indica la dimensione dell'iconografia di ogni carta nella pagina del libro."@it ;
    rdfs:domain <https://w3id.org/odi/Storycard> ;
    rdfs:range <https://w3id.org/odi/Dimension> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasImage> a owl:DatatypeProperty ;
    rdfs:label "Has image"@en,
        "Ha immagine"@it ;
    rdfs:comment "Links to a photo of each card of the tarot deck."@en,
        "Rimanda ad una foto di ogni carta del mazzo di tarocchi."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard> ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasIndirectRelationWith> a owl:ObjectProperty ;
    rdfs:label "Has indirect relation with"@en,
        "Ha una relazione indiretta con"@it ;
    rdfs:comment "This property connects a main character with an element of his story that has no direct link to him, but an indirect link through another card of the story."@en,
        "Questa proprietà collega un protagonista con un elemento della sua storia che non ha un legame diretto con lui, ma un legame indiretto attraverso un'altra carta della storia."@it ;
    rdfs:domain <https://w3id.org/odi/Representation> ;
    rdfs:range <https://w3id.org/odi/Representation> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasMeaningOf> a owl:ObjectProperty ;
    rdfs:label "Has meaning of"@en,
        "Ha significato di"@it ;
    rdfs:comment "It gives a short description of a card's meaning in a story."@en,
        "Fornisce una breve descrizione del significato di una carta in una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Representation> ;
    rdfs:range <https://w3id.org/odi/Meaning> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasName> a owl:DatatypeProperty ;
    rdfs:label "Has name"@en,
        "Ha nome"@it ;
    rdfs:comment "It indicates the specific name of the tarot deck and each of its cards."@en,
        "Indica il nome specifico del mazzo di tarocchi e di ciascuna delle sue carte."@it ;
    rdfs:domain <https://w3id.org/odi/TarotDeck> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasOriginalNumberOfCards> a owl:DatatypeProperty ;
    rdfs:label "Has original number of cards"@en,
        "Ha numero di carte originariamente"@it ;
    rdfs:comment "It indicates the original number of cards in the tarot deck."@en,
        "Indica il numero originale di carte del mazzo di tarocchi."@it ;
    rdfs:domain <https://w3id.org/odi/TarotDeck> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasPositionInTheText> a owl:DatatypeProperty ;
    rdfs:label "Has position in the text"@en,
        "Ha posizione nel testo"@it ;
    rdfs:comment "It indicates the order of appearance of each card in the different stories."@en,
        "Indica l'ordine di apparizione di ogni carta nelle diverse storie."@it ;
    rdfs:domain <https://w3id.org/odi/Storycard> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasPublicationDate> a owl:DatatypeProperty ;
    rdfs:label "Has publication date"@en,
        "Ha data di pubblicazione"@it ;
    rdfs:comment "This property connects the edition of reference with its date of publication."@en,
        "Questa proprietà collega l'edizione di riferimento con la sua data di pubblicazione."@it ;
    rdfs:domain <https://w3id.org/odi/Edition> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasSuit> a owl:ObjectProperty ;
    rdfs:label "Has suit"@en,
        "Ha seme"@it ;
    rdfs:comment "It indicates the suit of a card."@en,
        "Indica il seme di una carta."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard> ;
    rdfs:range <https://w3id.org/odi/Suit> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasTextualReference> a owl:DatatypeProperty ;
    rdfs:label "Has textual reference"@en,
        "Ha riferimento testuale"@it ;
    rdfs:comment "It reports in hooked brackets Calvin's words that give a more accurate description of each card."@en,
        "Riporta tra parentesi uncinate le parole di Calvino che danno una descrizione più accurata di ogni carta."@it ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasTitle> a owl:DatatypeProperty ;
    rdfs:label "Has title"@en,
        "Ha titolo"@it ;
    rdfs:comment "It shows both the title of the reference edition and of a chapter that is also the title of a story."@en,
        "Mostra sia il titolo dell'edizione di riferimento sia di un capitolo che è anche il titolo di una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Chapter>,
        <https://w3id.org/odi/Edition>,
        <https://w3id.org/odi/Story> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasTypology> a owl:ObjectProperty ;
    rdfs:label "Has typology"@en,
        "Ha tipologia"@it ;
    rdfs:comment "It indicates the typology of the tarot deck and of each of its cards."@en,
        "Indica la tipologia del mazzo di tarocchi e di ciascuna delle sue carte."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard>,
        <https://w3id.org/odi/TarotDeck> ;
    rdfs:range <https://w3id.org/odi/Typology> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/hasVariantName> a owl:DatatypeProperty ;
    rdfs:label "Has variant name"@en,
        "Ha variante del nome"@it ;
    rdfs:comment "It indicates the variant name of a card in a story."@en,
        "Indica il nome della variante di una carta presente in una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Storycard> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/hasVariantNameInTheText> a owl:DatatypeProperty ;
    rdfs:label "has variant name in the text"@en,
        "Ha variante del nome nel testo"@it ;
    rdfs:comment "It indicates which cards have a variant name in the book."@en,
        "Indica quali carte hanno una variante del nome nel libro."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard> ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/implies> a owl:ObjectProperty ;
    rdfs:label "Implies"@en,
        "Implica"@it ;
    rdfs:comment "An event implies one or more objects in a story."@en,
        "Un evento implica uno o più oggetti in una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Event> ;
    rdfs:range <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/includes> a owl:ObjectProperty ;
    rdfs:label "Includes"@en,
        "Include"@it ;
    rdfs:comment "This property shows a story included in a chapter."@en,
        "Questa proprietà mostra una storia inclusa in un capitolo."@it ;
    rdfs:domain <https://w3id.org/odi/Chapter> ;
    rdfs:range <https://w3id.org/odi/Story> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/isDefeatedBy> a owl:ObjectProperty ;
    rdfs:label "Is defeated by"@en,
        "È sconfitto da"@it ;
    rdfs:comment "This property indicates that a main character is defeated by another character in his story."@en,
        "Questa proprietà indica che un protagonista viene sconfitto da un altro personaggio nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/isHelpedBy> a owl:ObjectProperty ;
    rdfs:label "Is helped by"@en,
        "È aiutato da"@it ;
    rdfs:comment "This property indicates that a main character is helped by another character in his story."@en,
        "Questa proprietà indica che un protagonista è aiutato da un altro personaggio nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/isNumber> a owl:DatatypeProperty ;
    rdfs:label "Is number"@en,
        "È numero"@it ;
    rdfs:comment "It indicates the number representing each number card."@en,
        "Indica il numero che rappresenta ogni \"Numerale\"."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard> ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf owl:topDataProperty .

<https://w3id.org/odi/isRelatedWith> a owl:ObjectProperty ;
    rdfs:label "Is related with"@en,
        "È correlata a"@it ;
    rdfs:comment "This property connects two cards which have a general relation in a story."@en,
        "Questa proprietà collega due carte che hanno una relazione generale in una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Representation> ;
    rdfs:range <https://w3id.org/odi/Representation> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/isSpecifiedBy> a owl:ObjectProperty ;
    rdfs:label "Is specified by"@en,
        "È specificata da"@it ;
    rdfs:comment "It connects two cards where one specifies the other in a story."@en,
        "Collega due carte in cui una specifica l'altra in una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Representation> ;
    rdfs:range <https://w3id.org/odi/Representation> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/longsFor> a owl:ObjectProperty ;
    rdfs:label "Longs for"@en,
        "Desidera/Brama"@it ;
    rdfs:comment "It connects a main character and another character or an object desired by him in his story."@en,
        "Mette in relazione un protagonista e un altro personaggio o un oggetto da lui desiderato nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character>,
        <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/loses> a owl:ObjectProperty ;
    rdfs:label "Loses"@en,
        "Perde"@it ;
    rdfs:comment "It connects a main character and an object which is lost by him in his story."@en,
        "Mette in relazione un protagonista e un oggetto che ha perso nel corso della sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/marries> a owl:ObjectProperty ;
    rdfs:label "Marries"@en,
        "Sposa"@it ;
    rdfs:comment "This property indicates a main character who marries a character in his story."@en,
        "Questa proprietà indica un protagonista che sposa un personaggio nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Character> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/movingThrough> a owl:ObjectProperty ;
    rdfs:label "Moving through"@en,
        "Si muove per"@it ;
    rdfs:comment "It means that a main character is moving through a place in his story."@en,
        "Significa che un protagonista si muove in un luogo nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/FictionalPlace> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/placeOfPublication> a owl:ObjectProperty ;
    rdfs:label "Place of publication"@en,
        "Luogo di pubblicazione"@it ;
    rdfs:comment "This property connects the edition of reference with its place of publication."@en,
        "Questa proprietà collega l'edizione di riferimento con il suo luogo di pubblicazione."@it ;
    rdfs:domain <https://w3id.org/odi/Edition> ;
    rdfs:range <https://w3id.org/odi/Place> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/publisher> a owl:ObjectProperty ;
    rdfs:comment "Publisher"@en,
        "This property connects the edition of reference with its publishing house."@en,
        "Editore"@it,
        "Questa proprietà collega l'edizione di riferimento con la sua casa editrice."@it ;
    rdfs:domain <https://w3id.org/odi/Edition> ;
    rdfs:range <https://w3id.org/odi/PublishingHouse> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/receives> a owl:ObjectProperty ;
    rdfs:label "Receives"@en,
        "Riceve"@it ;
    rdfs:comment "It means that a main character receives something during his story."@en,
        "Significa che un protagonista riceve qualcosa durante la sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/represents> a owl:ObjectProperty ;
    rdfs:label "Represents"@en,
        "Rappresenta"@it ;
    rdfs:comment "This property shows a character as a person and a fictional place as a place."@en,
        "Mostra cosa rappresenta una carta in una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character>,
        <https://w3id.org/odi/FictionalPlace> ;
    rdfs:range <https://w3id.org/odi/Person>,
        <https://w3id.org/odi/Place> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/sameAs> a owl:ObjectProperty ;
    rdfs:label "Same as"@en,
        "È uguale a"@it ;
    rdfs:comment "This property indicates that two cards represent the same thing."@en,
        "Questa proprietà indica che due carte rappresentano la stessa cosa."@it ;
    rdfs:domain <https://w3id.org/odi/Representation> ;
    rdfs:range <https://w3id.org/odi/Representation> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/sees> a owl:ObjectProperty ;
    rdfs:label "Sees"@en,
        "Vede"@it ;
    rdfs:comment "It connects a main character with an event or object seen by him in his story."@en,
        "Mette in relazione un protagonista con un evento o un oggetto visto da lui nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/Event>,
        <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/speaksTo> a owl:ObjectProperty ;
    rdfs:label "Speaks to"@en,
        "Parla con"@it ;
    rdfs:comment "This property shows a main character speaking to another character in a story."@en,
        "Questa proprietà mostra un personaggio principale che parla a un altro personaggio di una storia."@it ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/specifies> a owl:ObjectProperty ;
    rdfs:label "Specifies"@en,
        "Specifica"@it ;
    rdfs:comment "This property specifies a card of the deck as a card of a story."@en,
        "Questa proprietà specifica una carta del mazzo come carta di una storia."@it ;
    rdfs:domain <https://w3id.org/odi/Storycard> ;
    rdfs:range <https://w3id.org/odi/DeckCard> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/trades> a owl:ObjectProperty ;
    rdfs:label "Trades"@en,
        "Baratta"@it ;
    rdfs:comment "This property indicates that a main character trades something in his story."@en,
        "Questa proprietà indica che un protagonista baratta qualcosa nella sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Character> ;
    rdfs:range <https://w3id.org/odi/InanimateObject> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/Dimension> a owl:Class ;
    rdfs:label "Dimension"@en,
        "Dimensione"@it ;
    rdfs:comment "Is the dimension of the card's iconography in the page of the book."@en,
        "È la dimensione dell'iconografia della carta nella pagina del libro."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/Meaning> a owl:Class ;
    rdfs:label "Meaning"@en,
        "Significato"@it ;
    rdfs:comment "Is a short meaning of what represent a card in a story."@en,
        "È un breve significato di ciò che rappresenta una carta in una storia."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/PublishingHouse> a owl:Class ;
    rdfs:label "Publishing house"@en,
        "Casa editrice"@it ;
    rdfs:comment "Is the publishing house of the edition of reference."@en,
        "È la casa editrice dell'edizione di riferimento"@it ;
    rdfs:subClassOf <https://w3id.org/odi/Edition> .

<https://w3id.org/odi/Suit> a owl:Class ;
    rdfs:label "Suit"@en,
        "Seme"@it ;
    rdfs:comment "Is the suit of a card."@en,
        "È il seme di una carta."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/Symbol> a owl:Class ;
    rdfs:label "Symbol"@en,
        "Simbolo"@it ;
    rdfs:comment "A symbol or an allegory of something expressed by a card."@en,
        "Un simbolo o un'allegoria di qualcosa espressi da una carta."@it ;
    rdfs:subClassOf <https://w3id.org/odi/Representation> .

<https://w3id.org/odi/Typology> a owl:Class ;
    rdfs:label "typology"@en,
        "Tipologia"@it ;
    rdfs:comment "Is the typology of the deck and of each card of the deck."@en,
        "È la tipologia del mazzo e di ogni carta del mazzo."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/isACardOf> a owl:ObjectProperty ;
    rdfs:label "Is a card of"@en,
        "È una carta di"@it ;
    rdfs:comment "It connects each card with its story."@en,
        "Collega ogni carta alla sua storia."@it ;
    rdfs:domain <https://w3id.org/odi/Storycard> ;
    rdfs:range <https://w3id.org/odi/Story> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/isContainedIn> a owl:ObjectProperty ;
    rdfs:label "Is contained in"@en,
        "È contenuta in"@it ;
    rdfs:comment "It connects each card with its tarot deck."@en,
        "Collega ogni carta al relativo mazzo di tarocchi."@it ;
    rdfs:domain <https://w3id.org/odi/DeckCard> ;
    rdfs:range <https://w3id.org/odi/TarotDeck> ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/odi/Person> a owl:Class ;
    rdfs:label "Person"@en,
        "Persona"@it ;
    rdfs:comment "Is a real person related to the book (e.g. the author of the book)."@en,
        "È una persona reale legata al libro (es. l'autore del libro)."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/Event> a owl:Class ;
    rdfs:label "Evento"@it ;
    rdfs:comment "An event that is carried out by somebody and/or implies something in a story."@en,
        "Event"@en,
        "Un avvenimento che viene eseguito da qualcuno e/o implica degli oggetti in una storia."@it ;
    rdfs:subClassOf <https://w3id.org/odi/Representation> .

<https://w3id.org/odi/FictionalPlace> a owl:Class ;
    rdfs:label "Fictional Place"@en,
        "Luogo ideale"@it ;
    rdfs:comment "A place where a character is moving through or arrives at in a story."@en,
        "Un luogo in cui un personaggio si muove o arriva in una storia."@it ;
    rdfs:subClassOf <https://w3id.org/odi/Representation> .

<https://w3id.org/odi/Place> a owl:Class ;
    rdfs:label "Place"@en,
        "Luogo"@it ;
    rdfs:comment "Is a real place related to the book (e.g. the current location of a card)."@en,
        "È un luogo reale legato al libro (es. la collocazione attuale di una carta)."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/Chapter> a owl:Class ;
    rdfs:label "Chapter"@en,
        "Capitolo"@it ;
    rdfs:comment "Is a chapter of the book that contains a story."@en,
        "È un capitolo del libro che contiene una storia."@it ;
    rdfs:subClassOf <https://w3id.org/odi/Edition> .

<https://w3id.org/odi/Story> a owl:Class ;
    rdfs:label "Story"@en,
        "Storia"@it ;
    rdfs:comment "A story that makes up the book."@en,
        "Una storia che compone il libro."@it ;
    rdfs:subClassOf <https://w3id.org/odi/Chapter> .

<https://w3id.org/odi/Storycard> a owl:Class ;
    rdfs:label "Story card"@en,
        "Carta nella storia"@it ;
    rdfs:comment "A card of a story that makes up the book."@en,
        "Una carta di una storia che compone il libro."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/Edition> a owl:Class ;
    rdfs:label "Edition"@en,
        "Edizione"@it ;
    rdfs:comment "It is the edition of the book taken as a reference."@en,
        "È l'edizione del libro presa come riferimento."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/InanimateObject> a owl:Class ;
    rdfs:label "Inanimate object"@en,
        "Oggetto inanimato"@it ;
    rdfs:comment "Every kind of an object in a story."@en,
        "Ogni tipo di oggetto in una storia."@it ;
    rdfs:subClassOf <https://w3id.org/odi/Representation> .

<https://w3id.org/odi/TarotDeck> a owl:Class ;
    rdfs:label "Tarot deck"@en,
        "Mazzo di tarocchi"@it ;
    rdfs:comment "Is a particular type of deck."@en,
        "È un tipo particolare di mazzo."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/DeckCard> a owl:Class ;
    rdfs:label "Deck card"@en,
        "Carta del mazzo"@it ;
    rdfs:comment "Is a card which belongs to the deck."@en,
        "È una carta che appartiene al mazzo."@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/Representation> a owl:Class ;
    rdfs:comment "Representation"@en,
        "The representation of a card."@en,
        "La rappresentazione di una carta."@it,
        "Rappresentazione"@it ;
    rdfs:subClassOf owl:Thing .

<https://w3id.org/odi/Character> a owl:Class ;
    rdfs:label "Character"@en,
        "Personaggio"@it ;
    rdfs:comment "A character of a story."@en,
        "Un personaggio di una storia."@it ;
    rdfs:subClassOf <https://w3id.org/odi/Representation> .

owl:topObjectProperty rdfs:subPropertyOf owl:topObjectProperty .

