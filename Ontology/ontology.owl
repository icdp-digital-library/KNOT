        <skos:scopeNote xml:lang="en">Uses values from http://publications.europa.eu/resource/authority/frequency.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/conformsTo -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/conformsTo">
