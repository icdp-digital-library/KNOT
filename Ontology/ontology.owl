    <!-- http://purl.org/dc/terms/format -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/format">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
        <rdfs:range rdf:resource="http://purl.org/dc/terms/MediaTypeOrExtent"/>
        <rdfs:comment xml:lang="en">This property refers to the file format of the Distribution, most likely the format of the downloadable form of a dataset. Usage Note: Uses values from the EU File Type authority table where possible (DCAT AP documentation).</rdfs:comment>
